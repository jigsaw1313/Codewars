    "def solution(number):\n",
    "    # Initialize the sum to zero\n",
    "    s = 0\n",
    "    # Check if the number is negative\n",
    "    if number < 0:\n",
    "        return 0\n",
    "    # Iterate over the natural numbers below the input number\n",
    "    for i in range(number):\n",
    "        # Check if the number is a multiple of 3 or 5\n",
    "        if i % 3 == 0 or i % 5 == 0:\n",
    "            # Add it to the sum\n",
    "            s += i\n",
    "    # Return the sum\n",
    "    return s"
   ]
